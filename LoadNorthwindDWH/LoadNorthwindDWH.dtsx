  public class DwService
  {
    private readonly DwContext _context;

    public DwService(string conn)
    {
      _context = new DwContext(conn);
    }

    public void CleanData()
    {
      _context.FactOrderDetails.DeleteAllOnSubmit(_context.FactOrderDetails);
      _context.DimCustomers.DeleteAllOnSubmit(_context.DimCustomers);
      _context.DimEmployees.DeleteAllOnSubmit(_context.DimEmployees);
      _context.DimProducts.DeleteAllOnSubmit(_context.DimProducts);
      _context.DimShippers.DeleteAllOnSubmit(_context.DimShippers);
      _context.SubmitChanges();
    }
    public void LoadCustomers(IEnumerable<ViewCustomers> customers)
    {
      foreach (var customer in customers)
      {
        var newCustomer = new DimCustomer
        {
          CustomerID = customer.CustomerID,
          CompanyName = customer.CompanyName,
          City = customer.City,
          ContactName = customer.ContactName,
          Country = customer.Country
        };
        _context.DimCustomers.InsertOnSubmit(newCustomer);
      }
      _context.SubmitChanges();
    }

    public void LoadEmployees(IEnumerable<ViewEmployees> employees)
    {
      foreach (var employee in employees)
      {
        var newEmployee = new DimEmployee
        {
          EmployeeID = employee.EmployeeID,
          FirstName = employee.FirstName,
          LastName = employee.LastName,
          City = employee.City,
          Country = employee.Country
        };
        _context.DimEmployees.InsertOnSubmit(newEmployee);
      }
      _context.SubmitChanges();
    }

    public void LoadProducts(IEnumerable<ViewProducts> products)
    {
      foreach (var product in products)
      {
        var newProduct = new DimProduct
        {
          ProductID = product.ProductID,
          ProductName = product.ProductName,
          CategoryName = product.CategoryName,
          UnitPrice = product.UnitPrice,
          SupplierName = product.SupplierName
        };
        _context.DimProducts.InsertOnSubmit(newProduct);
      }
      _context.SubmitChanges();
    }

    public void LoadShippers(IEnumerable<ViewShippers> shippers)
    {
      foreach (var shipper in shippers)
      {
        var newShipper = new DimShipper
        {
          ShipperID = shipper.ShipperID,
          CompanyName = shipper.CompanyName,
        };
        _context.DimShippers.InsertOnSubmit(newShipper);
      }
      _context.SubmitChanges();
    }

    public void LoadOrderDetails(IEnumerable<ViewOrderDetail> orderDetails)
    {
      foreach (var orderDetail in orderDetails)
      {
        var productKey = _context.DimProducts.FirstOrDefault(p => p.ProductID == orderDetail.ProductID)?.ProductKey ?? 0;
        var customerKey = _context.DimCustomers.FirstOrDefault(c => c.CustomerID == orderDetail.CustomerID)?.CustomerKey ?? 0;
        var employeeKey = _context.DimEmployees.FirstOrDefault(e => e.EmployeeID == orderDetail.EmployeeID)?.EmployeeKey ?? 0;
        var shipperKey = _context.DimShippers.FirstOrDefault(s => s.ShipperID == orderDetail.ShipperID)?.ShipperKey ?? 0;

        var newOrderDetail = new FactOrderDetail
        {
          OrderID = orderDetail.OrderID,
          ProductKey = productKey,
          CustomerKey = customerKey,
          EmployeeKey = employeeKey,
          ShipperKey = shipperKey,
          UnitPrice = orderDetail.UnitPrice,
          Quantity = orderDetail.Quantity
        };
        _context.FactOrderDetails.InsertOnSubmit(newOrderDetail);
      }
      _context.SubmitChanges();
    }

