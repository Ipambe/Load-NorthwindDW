  }

  private async Task LoadFactOrders(int orderID, int productID, string customerID, int employeeID, int shipperID, decimal unitPrice, int quantity)
  {
    try
    {
      using (var conn = new SqlConnection(this.Variables.ConnString))
      {
        using (var command = new SqlCommand("[load].[FactOrderDetail]", conn))
        {
          command.CommandType = CommandType.StoredProcedure;
          command.Parameters.AddWithValue("@OrderID", orderID);
          command.Parameters.AddWithValue("@ProductID", productID);
          command.Parameters.AddWithValue("@CustomerID", customerID);
          command.Parameters.AddWithValue("@EmployeeID", employeeID);
          command.Parameters.AddWithValue("@ShipperID", shipperID);
          command.Parameters.AddWithValue("@UnitPrice", unitPrice);
          command.Parameters.AddWithValue("@Quantity", quantity);

          await conn.OpenAsync();
          await command.ExecuteNonQueryAsync();
        }
      }
    }
    catch (Exception ex)
    {
      Console.WriteLine(ex.Message);
    }
  }
